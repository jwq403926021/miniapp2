.accident-collapse .van-collapse-item__title--expanded {
  font-weight: bold;
}
.accident-collapse .van-collapse-item__content{
  padding: 0 0 0 0;
}
.accident-collapse .van-collapse-item__content .van-cell__title,.van-cell__value{
  font-size: 13px!important;
}
.accident-collapse .van-collapse-item__content .van-cell__title{
  width: 100px!important;
  min-width: 100px!important;
  max-width: 100px!important;
}
.accident-detail-table-header{
  margin-left: 15px;
  margin-right: 15px;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
}
.accident-detail-table-body {
  height: 30px;
  margin-left: 15px;
  margin-right: 15px;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
}
.accident-detail-table-header .van-col {
  text-align: center;
  font-size: 14px;
  font-weight: bold;
  color: #4e4d50;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  line-height: 36px;
}
.accident-detail-table-body .van-col{
  text-align: center;
  border-bottom: 1px solid #ccc;
  line-height: 30px;
}
.accident-detail-table-body .van-col text{
  width: 100%;
  word-break: break-all;
  overflow: hidden;
  text-overflow: ellipsis;
  display: block;
  height: 30px;
}
.accident-detail-table-body .van-col input{
  line-height: 30px;
  height: 30px;
}
.addtext{
  background: #016eff;
  color: #fff;
  text-align: center;
  line-height: 30px;
  box-sizing: border-box;
}
.accident-input-cell-group .van-cell{
  padding: 5px 15px;
}
.accident-input-cell-group .van-cell .van-cell__title{
  line-height: 26px;
}

.mtb10{
  margin: 10px 0;
}

.add-item-popup{
  width: 100%;
  height: 90%;
}

.totalPrice{
  text-align: right;
  font-size: 14px;
  margin-top: 10px;
  margin-right: 10px;
  font-weight: bold;
}
.countnum{
  margin:10px 0 10px 15px;
}